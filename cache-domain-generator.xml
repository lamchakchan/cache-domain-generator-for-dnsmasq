<?xml version="1.0"?>
<Container version="2">
  <Name>cache-domain-generator</Name>
  <Repository>lamchakchan/cache-domain-generator-for-dnsmasq:latest</Repository>
  <Registry>https://github.com/lamchakchan/cache-domain-generator-for-dnsmasq/</Registry>
  <Network>br0</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project/>
  <Overview/>
  <Category/>
  <WebUI/>
  <TemplateURL>https://raw.githubusercontent.com/lamchakchan/cache-domain-generator-for-dnsmasq/master/cache-domain-generator.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/lamchakchan/cache-domain-generator-for-dnsmasq/master/images/cache-domain-generator.png</Icon>
  <ExtraParams>--hostname=cache-domain-generator</ExtraParams>
  <PostArgs/>
  <CPUset/>
  <DateInstalled/>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="Host dnsmasq directory" Target="/etc/dnsmasq.d" Default="/mnt/user/appdata/pihole/dnsmasq.d/" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/pihole/dnsmasq.d/</Config>
  <Config Name="Cron Expression" Target="CRON_EXPRESSION" Default="0 0 * * 0" Mode="" Description="Time interval for the refresh to run.  Defualts to weekly on Sunday." Type="Variable" Display="always" Required="false" Mask="false"/>
  <Config Name="Cache Domain Git Url" Target="CACHE_DOMAIN_GIT_URL" Default="https://github.com/uklans/cache-domains.git" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">https://github.com/uklans/cache-domains.git</Config>
  <Config Name="Lancache IP" Target="LANCACHE_IP" Default="" Mode="" Description="The IP or domain name to the Lancache server" Type="Variable" Display="always" Required="false" Mask="false">192.168.1.12</Config>
</Container>